#!/usr/bin/sed -f
#
# Oracle SQL Developer
#
# Configure data formats under preferences -> Database -> NLS
#   DD-MM-YYYY HH:MI:SS
#
# Tools -> Database Export
# Select only: DDL, pretty print, data (insert)
# Select only: tables, sequences
#
/CREATE.* TABLE .*\$.*/,/;$/ d
/CREATE TABLE "WORK_TMP/,/;$/ d
s/CONSTRAINT/XCONSTRAINT/g
/^--/d
/COMMENT ON TABLE/d
/^REM/d
/Insert into \w*\$/d
/CREATE SEQUENCE/s/ NOCACHE//g
/CREATE SEQUENCE/s/ NOORDER//g
/CREATE SEQUENCE/s/ NOCYCLE//g
/CREATE SEQUENCE/s/ MINVALUE [0-9]*//g
/CREATE SEQUENCE/s/ MAXVALUE [0-9]*//g
s/VARCHAR2/VARCHAR/g